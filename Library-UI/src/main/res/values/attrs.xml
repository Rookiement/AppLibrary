<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppFlowLayout">
        <attr name="flowMaxSelected" format="integer" />
        <attr name="flowSingleLine" format="boolean" />
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="flowGravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AppRefreshLayout">
        <attr name="ProgressSpinnerBackgroundColor" format="color" />
    </declare-styleable>
</resources>