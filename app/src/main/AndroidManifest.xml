<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.library.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.values.ValueColorActivity" />
        <activity android:name=".ui.values.ValueFontActivity" />

        <activity android:name=".ui.flow.AppFlowActivity" />

        <activity android:name=".ui.recycler.AppRecyclerActivity" />
        <activity android:name=".ui.recycler.RecyclerLinearDecorationActivity" />
        <activity android:name=".ui.recycler.RecyclerGridDecorationActivity" />
        <activity android:name=".ui.recycler.RecyclerMultiActivity" />

        <activity android:name=".ui.refresh.AppRefreshActivity" />
    </application>

</manifest>